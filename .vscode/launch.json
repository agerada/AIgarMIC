    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: rename_images.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/rename_images.py",
            "console": "integratedTerminal",
            "justMyCode": true, 
            "args": [
                "images/amikacin", "64"
            ]
        },
        {
            "name": "Python: clean_up_annotations.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/clean_up_annotations.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "annotations/clean"
            ]
        },
        {
            "name": "Python: main.py binary",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "images/2023-03-07_thick",
                "-m",
                "models_binary/growth_no_growth_2023-03-17",
                "models_binary/growth_poor_growth",
                "-t",
                "binary",
                "-o",
                "2023-03-17.csv",
                "-c"
            ]
        },
        {
            "name": "Python: train_binary_model.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train_binary_model.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "annotations/binary_clean_growth_no_growth",
                "-v",
                "-s",
                "models/",
                "-l",
                "-t",
                "annotations/testing_datasets/growth_no_growth"
            ]
        },
        {
            "name": "Python: main.py softmax",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "images/2023-03-07_thick",
                "-m",
                "model_archive/working_clean",
                "-t",
                "softmax",
                "-o",
                "2023-03-07.csv"
            ]
        },
        {
            "name": "Python: train_model.py",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "annotations/binary",
                "-v",
                "-s",
                "models/"
            ]
        },
        {
            "name": "Python: manual_annotator.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manual_annotator.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "images/2023-03-07_thick",
                "-o",
                "new_annotations/"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}